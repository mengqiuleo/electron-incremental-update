{
  "name": "electron-incremental-update",
  "version": "0.1.0",
  "description": "electron incremental update tools, powered by vite",
  "scripts": {
    "build": "tsup",
    "release": "tsup && bumpp"
  },
  "repository": "https://github.com/subframe7536/electron-incremental-update",
  "type": "module",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    },
    "./vite": {
      "require": "./dist/vite.cjs",
      "import": "./dist/vite.mjs"
    }
  },
  "typesVersions": {
    "*": {
      ".": [
        "dist/updater.d.ts"
      ],
      "vite": [
        "dist/vite.d.ts"
      ]
    }
  },
  "keywords": [
    "electron",
    "incremental update",
    "updater"
  ],
  "author": "",
  "devDependencies": {
    "@electron/asar": "^3.2.4",
    "@subframe7536/eslint-config": "^0.1.9",
    "@types/node": "^20.2.5",
    "asar": "^3.2.0",
    "bumpp": "^9.1.1",
    "electron": "^25.0.1",
    "eslint": "^8.42.0",
    "fs-jetpack": "^5.1.0",
    "tsup": "^6.7.0",
    "typescript": "^5.1.3",
    "vite": "^4.3.9"
  },
  "dependencies": {
    "ci-info": "^3.8.0"
  },
  "peerDependencies": {
    "@electron/asar": "*",
    "asar": "*"
  }
}