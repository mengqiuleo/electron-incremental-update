{
  "name": "electron-incremental-update",
  "version": "1.0.3",
  "description": "electron incremental update tools, powered by vite",
  "author": "subframe7536",
  "license": "MIT",
  "repository": "https://github.com/subframe7536/electron-incremental-update",
  "keywords": [
    "electron",
    "incremental update",
    "updater"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./vite": {
      "import": "./dist/vite.mjs",
      "require": "./dist/vite.js"
    },
    "./utils": {
      "import": "./dist/utils.mjs",
      "require": "./dist/utils.js"
    }
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "files": [
    "dist",
    "utils.d.ts",
    "utils.js",
    "vite.d.ts",
    "vite.js"
  ],
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup && node fix-module.js",
    "release": "pnpm test && pnpm run build && bumpp --all && npm publish",
    "test": "vitest --run",
    "lint": "eslint . --fix"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "peerDependencies": {
    "esbuild": "*",
    "vite-plugin-electron": "^0.15.6 || ^0.28"
  },
  "dependencies": {
    "@electron/asar": "^3.2.9",
    "@subframe7536/type-utils": "^0.1.6",
    "selfsigned": "^2.4.1"
  },
  "devDependencies": {
    "@subframe7536/eslint-config": "^0.6.0",
    "@types/node": "^20.12.5",
    "bumpp": "^9.4.0",
    "electron": "28.1.1",
    "eslint": "^8.57.0",
    "tsup": "^8.0.2",
    "typescript": "^5.4.4",
    "vite": "^5.2.8",
    "vite-plugin-electron": "^0.28.4",
    "vitest": "^1.4.0"
  },
  "pnpm": {
    "overrides": {
      "has": "npm:@nolyfill/has@latest"
    }
  }
}
