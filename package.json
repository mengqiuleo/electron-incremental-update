{
  "name": "electron-incremental-update",
  "author": "subframe7536",
  "version": "0.6.4",
  "description": "electron incremental update tools, powered by vite",
  "scripts": {
    "build": "tsup",
    "release": "pnpm test && pnpm run build && bumpp --all && npm publish",
    "test": "vitest --run"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "repository": "https://github.com/subframe7536/electron-incremental-update",
  "type": "module",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "default": "./dist/index.mjs",
        "types": "./dist/index.d.ts"
      },
      "require": {
        "default": "./dist/index.cjs",
        "types": "./dist/index.d.cts"
      }
    },
    "./vite": {
      "import": {
        "default": "./dist/vite.mjs",
        "types": "./dist/vite.d.ts"
      },
      "require": {
        "default": "./dist/vite.cjs",
        "types": "./dist/vite.d.cts"
      }
    }
  },
  "keywords": [
    "electron",
    "incremental update",
    "updater"
  ],
  "devDependencies": {
    "@electron/asar": "^3.2.4",
    "@subframe7536/eslint-config": "^0.1.9",
    "@types/node": "^20.3.2",
    "asar": "^3.2.0",
    "bumpp": "^9.1.1",
    "electron": "^25.2.0",
    "eslint": "^8.43.0",
    "fs-jetpack": "^5.1.0",
    "tsup": "^7.1.0",
    "typescript": "^5.1.5",
    "vite": "^4.3.9",
    "vitest": "^0.32.2"
  },
  "dependencies": {
    "@cyyynthia/jscert": "^0.1.2",
    "ci-info": "^3.8.0"
  }
}
